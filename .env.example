# Example environment variables
# Copy this file to `.env` and populate with your real keys for local development

# Google / Gemini API key used by the serverless function
API_KEY=your_gemini_api_key_here

# Server-side API key used by the client to authenticate requests to the function
# If omitted the function will fall back to an internal default value (unsafe for production)
TALC_API_KEY=TALC_API_KEY_SECURE_882910
